{
  "name": "webservices-nl/platform-connector",
  "description": "platform connector generator for Webservices.nl",
  "keywords": [
    "soap",
    "soapclient",
    "webservicesNl",
    "webservices.nl"
  ],
  "config": {
    "gitlab-domains": [
      "gitlab.fdmg.org"
    ],
    "preferred-install": "dist"
  },
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "WebservicesNL",
      "email": "tech@webservices.nl",
      "homepage": "http://www.webservices.nl"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "^5.6 || ^7",
    "ext-soap": "*",
    "ext-xml": "*",
    "ext-dom": "*",
    "guzzlehttp/guzzle": "^6",
    "psr/log": "^1.0",
    "webservices-nl/common": "^0",
    "webservices-nl/utils": "^1",
    "roave/security-advisories": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "mockery/mockery": "^0.9",
    "monolog/monolog": "^1 || ^2",
    "league/factory-muffin": "^2",
    "friendsofphp/php-cs-fixer": "^2 || ^3",
    "sensiolabs/security-checker": "^6.0",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "^3.5",
    "phpstan/phpstan": "^0.12.18"
  },
  "autoload": {
    "psr-4": {
      "WebservicesNl\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebservicesNl\\Test\\": "test/"
    }
  }
}
