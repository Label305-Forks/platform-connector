{
  "name": "webservices-nl/soap-client",
  "description": "Soap client wrapper for Webservices.nl written in PHP",
  "keywords": [
    "soap",
    "soapclient",
    "webservicesNl",
    "webservices.nl"
  ],
  "config": {
    "gitlab-domains": [
      "gitlab.fdmg.org"
    ],
    "preferred-install": "dist"
  },
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "WebservicesNL",
      "email": "tech@webservices.nl",
      "homepage": "http://www.webservices.nl"
    }
  ],
  "require": {
    "php": "~5.4 || ^7.0",
    "psr/http-message": "^1.0",
    "psr/log": "^1.0",
    "php-http/plugins": "^1.0",
    "webservices-nl/common": "0.0.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "mockery/mockery": "~0.9",
    "monolog/monolog": "^1.18",
    "guzzlehttp/guzzle": "^6",
    "league/factory-muffin": "^2.1",
    "league/factory-muffin-faker": "~1.0-beta",
    "php-http/client-implementation": "*",
    "php-http/guzzle6-adapter": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "WebservicesNl\\Connector\\": "src/Connector/",
      "WebservicesNl\\Soap\\": "src/Soap/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebservicesNl\\Test\\": "test/"
    }
  }
}
